[user]
	name = Shad Sharma
	email = shadanan@gmail.com
[push]
	default = current
	autoSetupRemote = true
[pull]
	rebase = true
[fetch]
	prune = true
[color]
	ui = auto
[alias]
	amend = commit --amend --no-edit
	dag = log --graph --abbrev-commit --decorate --date=relative --format=format:'%C(bold blue)%h%C(reset) -%C(auto)%d%C(reset) %C(bold yellow italic)%s%C(reset)%n%C(bold magenta)âž¥%C(reset) %C(dim white)%an%C(reset) <%C(ul)%ae%C(reset)> -%C(reset) %C(cyan)%aD%C(reset) %C(green)(%ar)%C(reset)'
[include]
		path = ~/.gitconfigenv
